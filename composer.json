{
    "name": "mahdimajidzadeh/persianalize",
    "type": "library",
    "description": "Laravel package for normalize Persian text",
    "keywords": [
        "persian",
        "laravel",
        "normalize"
    ],
    "homepage": "https://github.com/MahdiMajidzadeh/Persianalize",
    "license": "MIT",
    "authors": [
        {
            "name": "Mahdi Majidzadeh",
            "email": "mahdi@majidzadeh.ir",
            "homepage": "http://majidzadeh.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": ">=5.1",
        "php" : "~5.6|~7.0"
    },
    "autoload": {
        "psr-4": {
            "MahdiMajidzadeh\\Persianalize\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MahdiMajidzadeh\\Persianalize\\PersianalizeServiceProvider"
            ]
        }
    }
}
